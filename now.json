  "version": 2,
  "scope": "changesets",
  "env": {
    "APP_ID": "@prod-app-id",
    "WEBHOOK_SECRET": "@prod-webhook-secret",
    "PRIVATE_KEY": "@prod-private-key-base64-encoded",
    "SENTRY_DSN": "@prod-sentry-dsn"
